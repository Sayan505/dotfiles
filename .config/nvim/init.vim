call plug#begin()
Plug 'andweeb/presence.nvim'
call plug#end()

syntax off

set noautoindent
set nosmartindent
set nocindent


set tabstop=4
set softtabstop=0
set shiftwidth=4
set expandtab

let g:loaded_matchparen=1

set belloff=all

set number

set noswapfile
set nobackup

set nowrap

set relativenumber

set incsearch
set ignorecase
set smartcase
set nohlsearch

set termguicolors
set background=dark

set guicursor=n:hor20,i:ver25,c:ver25,a:blinkon1

set shortmess=I
